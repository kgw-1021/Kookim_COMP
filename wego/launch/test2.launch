
<launch>
    <!-- <param name="use_sim_time" value="true"/> -->
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
      <!-- <remap from="scan" to="base_scan"/> -->
      <param name="map_update_interval" value="5.0"/>
      <param name="maxUrange" value="3.0"/>
      <param name="maxRange" value="20.0"/>
      <param name="sigma" value="5"/>
      <param name="kernelSize" value="10"/>
      <param name="lstep" value="1"/>
      <param name="astep" value="1"/>
      <param name="iterations" value="7"/>
      <param name="lsigma" value="5"/>
      <param name="ogain" value="1"/>
      <param name="minimunScore" value="50"/>
      <param name="lskip" value="2"/>
      <param name="srr" value="0.01"/>
      <param name="srt" value="0.02"/>
      <param name="str" value="0.01"/>
      <param name="stt" value="0.02"/>
      <param name="linearUpdate" value="1.0"/>
      <param name="angularUpdate" value="10"/>
      <param name="temporalUpdate" value="-1"/>
      <param name="resampleThreshold" value="50"/>
      <param name="particles" value="60"/>
      <param name="xmin" value="-50.0"/>
      <param name="ymin" value="-50.0"/>
      <param name="xmax" value="50.0"/>
      <param name="ymax" value="50.0"/>
      <param name="delta" value="0.01"/>
      <param name="llsamplerange" value="0.01"/>
      <param name="llsamplestep" value="0.01"/>
      <param name="lasamplerange" value="0.005"/>
      <param name="lasamplestep" value="0.005"/>
      
    </node>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find wego)/rviz/slam.rviz" />
</launch>